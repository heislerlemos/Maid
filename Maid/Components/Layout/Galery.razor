@foreach (var maid in maids)
{
    


<div class="card">
    <div class="card-image">
        <figure class="image is-4by3">
            <img src="https://merecrute.com/wp-content/uploads/2017/05/empregada-domestica.jpg"
                 alt="Placeholder image" />
        </figure>
    </div>
    <div class="card-content">
        <div class="media">
            <div class="media-left">
                <figure class="image is-48x48">
                    <img src="https://bulma.io/assets/images/placeholders/96x96.png"
                         alt="Placeholder image" />
                </figure>
            </div>
            <div class="media-content">
                    <p class="title is-4">Nome : @maid.Nome</p>
                    <p class="title is-3">Reputação: @maid.Idade</p>
                    <p class="subtitle is-6">Morada : @maid.Address</p>
                    <p class="subtitle is-5">Email : @maid.Email</p>
            </div>
        </div>

        <div class="content">
            5 Anos de Experiencia
          
            <br />
                <time datetime="2016-1-1">Numero de telefone : @maid.Telefone</time>
        </div>
    </div>
</div>
}

@code {

    private List<AddMaid> maids = new();
    string newNameMaid = "";
    string newAddressMaid = "";
    int newIdadeMaid = 0;
    string newTelefoneMaid = "";
    string newEmailMaid = "";



}
